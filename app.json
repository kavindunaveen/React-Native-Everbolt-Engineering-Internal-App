{
  "expo": {
    "name": "Everbolt Engineering",
    "slug": "everbolt-app",
    "description": "Everbolt Engineering internal app",
    "version": "2.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "everboltapp",
    "deeplinks": ["everboltapp://"],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.everboltit.everboltapp",
      "newArchEnabled": false
    },
    "android": {
      "versionCode": 14,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.everboltit.everboltapp",
      "permissions": [
        "WRITE_CONTACTS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "everbolt.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "everboltapp"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "newArchEnabled": false
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "28a72db4-d19a-4ea1-b693-a8c76e5a032e"
      }
    },
    "plugins": [
      "expo-secure-store",
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/28a72db4-d19a-4ea1-b693-a8c76e5a032e",
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes"
  }
}
